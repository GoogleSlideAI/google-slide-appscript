{
  "name": "slide-apps-script",
  "version": "3.0.0",
  "type": "module",
  "description": "Starter project for using React with Google Slides Apps Script",
  "license": "ISC",
  "scripts": {
    "dev": "vite",
    "lint": "eslint .",
    "test:integration": "jest --forceExit test/local-development.test",
    "test:integration:extended": "cross-env IS_EXTENDED=true jest --forceExit test/local-development.test",
    "test:integration:extended:ci-reporter": "cross-env IS_EXTENDED=true jest --forceExit || node test/utils/image-reporter-standalone.js",
    "login": "clasp login",
    "setup": "rimraf .clasp.json && mkdirp dist && clasp create --type slides --title \"My React Project\" --rootDir ./dist && move dist\\.clasp.json .\\.clasp.json",
    "open": "clasp open --addon",
    "push": "clasp push",
    "setup:https": "mkdirp certs && mkcert -key-file ./certs/key.pem -cert-file ./certs/cert.pem localhost 127.0.0.1",
    "build:dev": "tsc && vite build --mode development",
    "build": "tsc && vite build --mode production",
    "deploy:dev": "yarn build:dev && yarn push",
    "deploy": "yarn build && yarn push",
    "start": "yarn deploy:dev && yarn dev"
  },
  "keywords": [
    "react",
    "google",
    "apps",
    "script",
    "slides"
  ],
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "@chatscope/chat-ui-kit-react": "^2.0.3",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^6.3.1",
    "@mui/material": "^6.3.1",
    "@mui/x-data-grid": "^7.23.6",
    "@react-oauth/google": "^0.12.1",
    "@tanstack/react-query": "^5.64.0",
    "@toolpad/core": "^0.11.0",
    "@types/canvas-confetti": "^1.9.0",
    "@types/google-apps-script": "^1.0.91",
    "axios": "^1.7.9",
    "canvas-confetti": "^1.9.3",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "formik": "^2.4.6",
    "framer-motion": "^11.18.0",
    "gas-client": "^1.1.1",
    "hast-util-to-text": "^4.0.2",
    "lucide-react": "^0.470.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.4.0",
    "react-bootstrap-icons": "^1.11.5",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.3",
    "react-router-dom": "^7.1.1",
    "react-transition-group": "^4.4.2",
    "rehype-highlight": "^7.0.2",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "strip-markdown": "^6.0.0",
    "yup": "^1.6.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.6",
    "@google/clasp": "^2.4.2",
    "@types/expect-puppeteer": "^5.0.0",
    "@types/jest-environment-puppeteer": "^5.0.2",
    "@types/node": "^20.11.30",
    "@types/puppeteer": "^5.4.6",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.19",
    "aws-sdk": "^2.1106.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-googleappsscript": "^1.0.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "gas-types-detailed": "^1.1.2",
    "jest": "^28.1.1",
    "jest-environment-node": "^28.1.1",
    "jest-image-snapshot": "^5.1.0",
    "postcss": "^8.4.38",
    "postcss-preset-env": "^9.5.4",
    "prettier": "^3.3.3",
    "puppeteer": "^14.3.0",
    "puppeteer-extra": "^3.2.3",
    "puppeteer-extra-plugin-stealth": "^2.9.0",
    "rollup": "^4.18.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-singlefile": "^2.0.1",
    "vite-plugin-static-copy": "^1.0.1",
    "vite-plugin-svgr": "^4.3.0"
  }
}
